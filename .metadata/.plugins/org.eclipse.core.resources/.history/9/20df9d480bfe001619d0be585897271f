package app;

//import org.joda.time.LocalDate;

public class Planner {

	public static void run(){
		makeDates();
		System.out.println("words");
	}
	
	public static void makeDates(){
		for(int i = 0; i < 12; i++){
			for(int j = 0; j < 32; j++){
				if(i == 4 || i == 6 || i == 9 || i == 11 && j < 31){
					j = 32;
				}else if(i == 2 && j < 29){
					j = 32;
				}else{
//					LocalDate date = new LocalDate(2016, i, j);
				}
			}
		}
	}

}
